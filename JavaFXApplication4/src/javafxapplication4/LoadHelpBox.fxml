<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="903.0" prefWidth="1087.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxapplication4.LoadHelpBoxController">
   <children>
      <Label layoutX="65.0" layoutY="77.0" text="Here you can contact us for any queries or Problems related to APP" textFill="#8d0478" wrapText="true">
         <font>
            <Font name="Bookman Old Style Bold" size="26.0" />
         </font></Label>
      <Label layoutX="213.0" layoutY="256.0" text="Enter your ID" textFill="#bf0000">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <Label layoutX="182.0" layoutY="313.0" text="Enter your name" textFill="#bf0000">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <Label layoutX="291.0" layoutY="473.0" text="Describe your problem in details" textFill="#bf0000">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <TextField fx:id="IDforHelpFxID" layoutX="407.0" layoutY="259.0" promptText="ID" />
      <TextField fx:id="NameForHelpFxID" layoutX="408.0" layoutY="316.0" promptText="Name" />
      <TextArea fx:id="ProblemDetailsFxId" layoutX="212.0" layoutY="527.0" prefHeight="200.0" prefWidth="579.0" promptText="            Write in details">
         <font>
            <Font size="25.0" />
         </font>
      </TextArea>
      <Label layoutX="182.0" layoutY="363.0" text="Problem Subject" textFill="#bf0000">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <TextField fx:id="SubjectOfHelpFxID" layoutX="407.0" layoutY="365.0" promptText="Subject" />
      <Label layoutX="333.0" layoutY="136.0" text="We'll try our best to help" textFill="#8d0478">
         <font>
            <Font name="Bookman Old Style Bold" size="26.0" />
         </font>
      </Label>
      <Button layoutX="446.0" layoutY="749.0" mnemonicParsing="false" onAction="#SubmitProblemButtonClick" style="-fx-background-color: black;" text="Submit" textFill="WHITE">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Button>
      <Label fx:id="submitMessageFxID" layoutX="287.0" layoutY="813.0" prefHeight="79.0" prefWidth="360.0">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
